# Default environment variables for local development
# Copy this to .env and customize as needed
# This file is committed to git and should NOT contain secrets

# Database (local development with docker compose)
POSTGRES_DB=privfinos
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/privfinos

# API Configuration
PORT=3001
NODE_ENV=development
LOG_LEVEL=info

# CORS - Frontend dev server
CORS_ORIGIN=http://localhost:5173,http://localhost:5174
